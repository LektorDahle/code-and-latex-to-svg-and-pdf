{
    "schema_version": 1,
    "styles": {
        "default": {
            "font_family": "Clincher Mono",
            "font_size_pt": 12,
            "color": "#280064",
            "weight": 400
        },
        "comment": {
            "font_family": "Clincher Mono",
            "font_size_pt": 12,
            "color": "#aaaaaa",
            "weight": 400
        },
        "SomeThing": {
            "font_family": "Arial",
            "font_size_pt": 12,
            "color": "#000000",
            "weight": 400
        },
        "New Style": {
            "font_family": "Arial",
            "font_size_pt": 12,
            "color": "#000000",
            "weight": 400
        },
        "SSSS": {
            "font_family": "Arial",
            "font_size_pt": 12,
            "color": "#000000",
            "weight": 800
        }
    },
    "rules": {
        "strings": {
            "language": "Python",
            "start": "\"",
            "end": "\"",
            "multiline": true,
            "style": "comment",
            "priority": 2
        },
         "comments": {
            "start": "#",
            "end": "\n",
            "multiline": false,
            "style": "default",
            "priority": 2
        }
    },
    "wordRules": {
        "import": {
            "style": "comment",
            "priority": 1
        },
        "return": {
            "style": "default",
            "priority": 1
        }
    }
}